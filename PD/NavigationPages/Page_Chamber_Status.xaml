<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PD.NavigationPages"
             xmlns:uc="clr-namespace:PD.UI"
             xmlns:vm="clr-namespace:PD.ViewModel"
             xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
             x:Class="PD.NavigationPages.Page_Chamber_Status"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">

    <UserControl.Resources>
        <Style x:Key="ListBoxStyle1"
               TargetType="{x:Type ListBox}">
            <Setter Property="Background"
                    Value="{DynamicResource MahApps.Brushes.ThemeBackground}" />
            <Setter Property="BorderBrush"
                    Value="{DynamicResource MahApps.Brushes.ThemeForeground}" />
            <Setter Property="BorderThickness"
                    Value="0" />
            <Setter Property="HorizontalContentAlignment"
                    Value="Stretch" />
            <Setter Property="ScrollViewer.CanContentScroll"
                    Value="True" />
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility"
                    Value="Auto" />
            <Setter Property="ScrollViewer.PanningMode"
                    Value="Both" />
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility"
                    Value="Auto" />
            <Setter Property="SnapsToDevicePixels"
                    Value="True" />
            <Setter Property="Stylus.IsFlicksEnabled"
                    Value="False" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListBox}">
                        <Border x:Name="Border"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding mah:ControlsHelper.CornerRadius}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                            <ScrollViewer CanContentScroll="{TemplateBinding ScrollViewer.CanContentScroll}"
                                          Focusable="False"
                                          HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                                          Padding="{TemplateBinding Padding}"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                          VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                                <ItemsPresenter SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                            </ScrollViewer>
                        </Border>
                        <ControlTemplate.Triggers>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsGrouping"
                                               Value="True" />
                                    <Condition Property="VirtualizingPanel.IsVirtualizingWhenGrouping"
                                               Value="False" />
                                </MultiTrigger.Conditions>
                                <Setter Property="ScrollViewer.CanContentScroll"
                                        Value="False" />
                            </MultiTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="VerticalContentAlignment"
                    Value="Center" />
            <Style.Triggers>
                <Trigger Property="IsEnabled"
                         Value="False">
                    <Setter Property="BorderBrush"
                            Value="{DynamicResource MahApps.Brushes.Gray9}" />
                </Trigger>
                <Trigger Property="VirtualizingPanel.IsVirtualizing"
                         Value="True">
                    <Setter Property="ItemsPanel">
                        <Setter.Value>
                            <ItemsPanelTemplate>
                                <VirtualizingStackPanel VirtualizingPanel.IsVirtualizing="True"
                                                        IsItemsHost="True"
                                                        VirtualizingPanel.IsVirtualizingWhenGrouping="True"
                                                        VirtualizingPanel.VirtualizationMode="Recycling" />
                            </ItemsPanelTemplate>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="CheckBoxStyle1"
               TargetType="{x:Type CheckBox}">
            <Setter Property="Background"
                    Value="{DynamicResource MahApps.Brushes.CheckBox.BackgroundUnchecked}" />
            <Setter Property="BorderBrush"
                    Value="{DynamicResource MahApps.Brushes.CheckBox.BorderBrushUnchecked}" />
            <Setter Property="BorderThickness"
                    Value="0" />
            <Setter Property="FontFamily"
                    Value="{DynamicResource MahApps.Fonts.Family.Control}" />
            <Setter Property="FontSize"
                    Value="{DynamicResource MahApps.Font.Size.Content}" />
            <Setter Property="Foreground"
                    Value="{DynamicResource MahApps.Brushes.CheckBox.ForegroundUnchecked}" />
            <Setter Property="HorizontalContentAlignment"
                    Value="Left" />
            <Setter Property="OverridesDefaultStyle"
                    Value="True" />
            <Setter Property="Padding"
                    Value="6,0,0,0" />
            <Setter Property="SnapsToDevicePixels"
                    Value="True" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type CheckBox}">
                        <Grid>
                            <Border x:Name="RootBorder"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Background="{TemplateBinding Background}"
                                    CornerRadius="{TemplateBinding mah:ControlsHelper.CornerRadius}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                            <Grid Margin="{TemplateBinding BorderThickness}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition x:Name="LeftCol"
                                                      Width="Auto" />
                                    <ColumnDefinition x:Name="RightCol"
                                                      Width="0*" />
                                </Grid.ColumnDefinitions>
                                <Grid x:Name="Check"
                                      Grid.Column="0"
                                      HorizontalAlignment="Center"
                                      Height="{TemplateBinding mah:CheckBoxHelper.CheckSize}"
                                      SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                      VerticalAlignment="Center"
                                      Width="{TemplateBinding mah:CheckBoxHelper.CheckSize}">
                                    <Rectangle x:Name="NormalRectangle"
                                               Fill="{TemplateBinding mah:CheckBoxHelper.CheckBackgroundFillUnchecked}"
                                               Stroke="{TemplateBinding mah:CheckBoxHelper.CheckBackgroundStrokeUnchecked}"
                                               StrokeThickness="{TemplateBinding mah:CheckBoxHelper.CheckStrokeThickness}"
                                               UseLayoutRounding="False" />
                                    <ContentControl x:Name="CheckGlyph"
                                                    ContentTemplate="{TemplateBinding mah:CheckBoxHelper.CheckGlyphUncheckedTemplate}"
                                                    Content="{TemplateBinding mah:CheckBoxHelper.CheckGlyphUnchecked}"
                                                    Foreground="{TemplateBinding mah:CheckBoxHelper.CheckGlyphForegroundUnchecked}"
                                                    Focusable="False"
                                                    IsTabStop="False" />
                                </Grid>
                                <ContentPresenter x:Name="ContentPresenter"
                                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                                  Content="{TemplateBinding Content}"
                                                  Grid.Column="1"
                                                  Visibility="Collapsed"
                                                  ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                                  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                  Margin="{TemplateBinding Padding}"
                                                  RecognizesAccessKey="{TemplateBinding mah:ControlsHelper.RecognizesAccessKey}"
                                                  SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                            </Grid>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="False" />
                                    <Condition Property="IsMouseOver"
                                               Value="False" />
                                    <Condition Property="IsPressed"
                                               Value="False" />
                                    <Condition Property="IsEnabled"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUncheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="False" />
                                    <Condition Property="IsMouseOver"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUncheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundUncheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundUncheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillUncheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeUncheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundUncheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushUncheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="False" />
                                    <Condition Property="IsPressed"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUncheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundUncheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundUncheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillUncheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeUncheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundUncheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushUncheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="False" />
                                    <Condition Property="IsEnabled"
                                               Value="False" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUnchecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphUncheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundUncheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundUncheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillUncheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeUncheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundUncheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushUncheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="True" />
                                    <Condition Property="IsMouseOver"
                                               Value="False" />
                                    <Condition Property="IsPressed"
                                               Value="False" />
                                    <Condition Property="IsEnabled"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphCheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="True" />
                                    <Condition Property="IsMouseOver"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphCheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundCheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundCheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillCheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeCheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundCheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushCheckedMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="True" />
                                    <Condition Property="IsPressed"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphCheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundCheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundCheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillCheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeCheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundCheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushCheckedPressed), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="True" />
                                    <Condition Property="IsEnabled"
                                               Value="False" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphChecked), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphCheckedTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundCheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundCheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillCheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeCheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundCheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushCheckedDisabled), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="{x:Null}" />
                                    <Condition Property="IsMouseOver"
                                               Value="False" />
                                    <Condition Property="IsPressed"
                                               Value="False" />
                                    <Condition Property="IsEnabled"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphIndeterminateTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="{x:Null}" />
                                    <Condition Property="IsMouseOver"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphIndeterminateTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundIndeterminateMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Opacity"
                                        TargetName="CheckGlyph"
                                        Value="1" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundIndeterminateMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillIndeterminateMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeIndeterminateMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Background"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BackgroundIndeterminateMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="BorderBrush"
                                        TargetName="RootBorder"
                                        Value="{Binding (mah:CheckBoxHelper.BorderBrushIndeterminateMouseOver), RelativeSource={RelativeSource TemplatedParent}}" />
                            </MultiTrigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsChecked"
                                               Value="{x:Null}" />
                                    <Condition Property="IsPressed"
                                               Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Content"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphIndeterminate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="ContentTemplate"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphIndeterminateTemplate), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Foreground"
                                        TargetName="CheckGlyph"
                                        Value="{Binding (mah:CheckBoxHelper.CheckGlyphForegroundIndeterminatePressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="TextElement.Foreground"
                                        TargetName="ContentPresenter"
                                        Value="{Binding (mah:CheckBoxHelper.ForegroundIndeterminatePressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Fill"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundFillIndeterminatePressed), RelativeSource={RelativeSource TemplatedParent}}" />
                                <Setter Property="Stroke"
                                        TargetName="NormalRectangle"
                                        Value="{Binding (mah:CheckBoxHelper.CheckBackgroundStrokeIndeterminatePressed), RelativeSource={RelativeSource TemplatedParent}}" />
