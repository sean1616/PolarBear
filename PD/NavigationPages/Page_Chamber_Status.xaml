                                              HorizontalAlignment="Stretch"
                                              Btn_station_name_Click="uc_chambr_Btn_station_name_Click" />
                        <Viewbox Grid.Column="1"
                                 HorizontalAlignment="Left"
                                 Margin="0,5,5,5">
                            <ListBox ItemsSource="{Binding FailureMode_Models}"
                                     Style="{DynamicResource ListBoxStyle1}"
                                     ItemContainerStyle="{DynamicResource ListBoxItemStyle1}"
                                     Background="Transparent"
                                     Grid.Column="1">
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <VirtualizingStackPanel IsItemsHost="True"
                                                                Orientation="Horizontal" />
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Grid Width="75"
                                              Height="80"
                                              Margin="0,0,0,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                            </Grid.RowDefinitions>
                                            <Grid Background="#FFACFBF4">
                                                <Viewbox>
                                                    <TextBlock Text="{Binding ch_name}"
                                                               FontFamily="Segoe Print"
                                                               HorizontalAlignment="Center"
                                                               VerticalAlignment="Center" />
                                                </Viewbox>
                                            </Grid>
                                            <Grid Background="#FFE3F7F6"
                                                  Grid.Row="1">
                                                <Viewbox>
                                                    <TextBlock Text="{Binding ch_UFV}"
                                                               FontFamily="Segoe Print"
                                                               HorizontalAlignment="Center"
                                                               VerticalAlignment="Center" />
                                                </Viewbox>
                                            </Grid>
                                            <Border Grid.Row="2"
                                                    Background="White">
                                                <CheckBox IsChecked="{Binding is_PDFail}"
                                                          HorizontalAlignment="Center"
                                                          VerticalAlignment="Center"
                                                          Style="{DynamicResource CheckBoxStyle1}"
                                                          Background="White" />
                                            </Border>
                                            <Border Grid.Row="3"
                                                    Background="White">
                                                <CheckBox IsChecked="{Binding is_VoltFail}"
                                                          HorizontalAlignment="Center"
                                                          VerticalAlignment="Center"
                                                          IsEnabled="False"
                                                          Style="{DynamicResource CheckBoxStyle1}"
                                                          Background="White" />
                                            </Border>
                                        </Grid>
                                    </DataTemplate>
